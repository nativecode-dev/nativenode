{
  "all": true,
  "exclude": [
    "node_modules",
    "**/src/**/*.spec.ts",
    "packages/*/src/**/index.ts"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "core/*/src/**/*.ts",
    "packages/*/src/**/*.ts"
  ],
  "instrument": true,
  "report-dir": ".cache/coverage",
  "reporter": "text",
  "sourceMap": true,
  "temp-directory": ".cache/nyc",
  "instrumentation": {
    "excludes": [
      "**/node_modules/**"
    ]
  }
}
