{
  "command": {
    "publish": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "package-lock.json",
        "package.json"
      ],
      "loglevel": "verbose",
      "message": "[skip ci]: publish",
      "yes": true
    },
    "run": {
      "stream": true
    }
  },
  "loglevel": "error",
  "npmClientArgs": [
    "--no-progress",
    "--silent"
  ],
  "packages": [
    "binaries/*",
    "packages/*"
  ],
  "version": "independent"
}
